<script>
  document.addEventListener("DOMContentLoaded", function() {
    const hamburgerBtn = document.getElementById("hamburgerBtn");
    const closeMenuBtn = document.getElementById("closeMenuBtn");
    const mobileMenuContainer = document.getElementById("mobileMenu");
    const searchForm = document.getElementById('searchForm')
    const navLinksContainer = document.getElementById("navLinks");
    const mainLogo = document.getElementById('mainLogo');

    
    const menuItems = [
    { href: "/", text: "Getting Started" },
    { href: "/architecture", text: "Architecture" },
    { href: "/platform", text: "Platform" },
    { href: "/reference", text: "Reference" },
    { href: "/integrations", text: "Integrations" },
    { href: "/contribute", text: "Contribute" },
    { href: "/blog", text: "Blog" }
    ];

    function renderNavLinks() {
      menuItems.forEach(item => {
        const link = document.createElement("a");
        link.href = item.href;
        link.classList.add("inline-flex", "items-center", "font-inter", "leading-[28px]", "text-base", "font-normal");
        if (location.pathname === item.href) {
          link.classList.add("text-brand");
        } else {
          link.classList.add("text-headLines", "transition", "duration-500", "hover:text-brand");
        }
        link.textContent = item.text;
        navLinksContainer.appendChild(link);
      });
    }

    renderNavLinks();

    let open = false;

    function updateLogoClasses() {
        if (open) {
          mainLogo.setAttribute('class', 'w-[171px] h-[32px] ml-2 md:ml-0');
        } else {
          mainLogo.setAttribute('class', 'w-[87px] h-[16px] md:w-[171px] md:h-[32px] md:ml-0');
        }
      }

    function toggleMobileMenu() {
      open = !open;
      mobileMenuContainer.classList.toggle("hidden", !open);
      hamburgerBtn.classList.toggle("hidden", open);
      closeMenuBtn.classList.toggle("hidden", !open);
      searchForm.classList.toggle('hidden', open) 
      document.body.classList.toggle('overflow-hidden', open);

      updateLogoClasses()
    }

    hamburgerBtn.addEventListener('click', toggleMobileMenu);
    closeMenuBtn.addEventListener('click', toggleMobileMenu);
  });
</script>